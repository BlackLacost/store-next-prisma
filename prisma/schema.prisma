datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Product {
  id    Int    @id @default(autoincrement())
  title String @unique @db.VarChar(255)
  price Int
}
